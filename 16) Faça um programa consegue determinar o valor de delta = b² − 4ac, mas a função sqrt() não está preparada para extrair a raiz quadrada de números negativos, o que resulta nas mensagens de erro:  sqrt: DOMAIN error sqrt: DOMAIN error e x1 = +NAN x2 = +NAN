#include <stdio.h>
#include <math.h>
int main()
{
  int a,b,c,delta,x1,x2;
  printf("Digite o valor de a:");
  scanf("%i", &a);
  printf("Digite o valor de b:");
  scanf("%i", &b);
  printf("Digite o valor de c:");
  scanf("%i", &c);

  delta = (b*b) - 4*a*c;
  x1 = (-b + sqrt(delta)) / (2*a);
  x2 = (-b - sqrt(delta)) / (2*a);
  printf("x1 = %i\n",x1);
  printf("x2 = %i\n", x2);


  return 0;
}
